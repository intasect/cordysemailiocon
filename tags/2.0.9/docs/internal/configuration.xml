<?xml version="1.0" encoding="UTF-8"?>
<tns:configuration xmlns:tns="http://emailioconnector.coe.cordys.com/2.0/configuration" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://emailioconnector.coe.cordys.com/2.0/configuration configuration.xsd ">
  <tns:emailboxes>
    <emailbox>
      <name></name>
      <host></host>
      <port></port>
      <type></type>
      <username></username>
      <password></password>
      <pollinterval></pollinterval>
      <folders>
        <folder></folder>
      </folders>
      <triggers>
        <trigger appliesTo="">
          <name></name>
          <description></description>
          <combineemails></combineemails>
          <rules>
            <rule section="">
              <pattern type="">
                <value></value>
                <name></name>
                <replacement>
                  <token>
                    <name></name>
                    <value src=""/>
                  </token>
                </replacement>
                <store>
                  <token>
                    <name></name>
                    <value></value>
                  </token>
                </store>
              </pattern>
            </rule>
          </rules>
          <message>
            <method></method>
            <namespace></namespace>
            <user></user>
            <sync></sync>
            <repeatingxpath></repeatingxpath>
            <organization></organization>
            <soapdn></soapdn>
            <timeout></timeout>
            <mappings>
              <mapping>
                <source></source>
                <value src=""/>
              </mapping>
            </mappings>
          </message>
        </trigger>
      </triggers>
    </emailbox>
  </tns:emailboxes>
  <maxworkers></maxworkers>
</tns:configuration>
